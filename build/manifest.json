{
	"manifest_version": 2,
	"version": "0.1",
	"name": "LikeAPinboard",
	"description": "LikeAPinboard Add Pin Tool",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"tabs",
        "http://*/*",
        "https://*/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"page_action": {
		"default_icon": "icon_default.png",
		"default_title": "LikeAPinboard",
		"default_popup": "popup.html"
	}
}
